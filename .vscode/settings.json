{
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "eslint.enable": true,
  "eslint.alwaysShowStatus": true,
  "eslint.workingDirectories": [
    "backend",
    "frontend"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "search.exclude": {
    "**/node_modules": true
  },
}
